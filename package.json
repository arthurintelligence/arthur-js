{
  "name": "@goarthur/arthur-js",
  "version": "0.0.1",
  "description": "Arthur's Client-side SDK",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/philippefutureboy/arthur-js",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "lint": "tslint --project . && prettier \"src/**/*.ts\" --list-different",
    "lint:fix": "tslint --fix --project . && prettier \"src/**/*.ts\" --write",
    "test": "node scripts/test.js \".*\\.test\\.(js|jsx|ts|tsx)\"",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash build test",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('@goarthur/arthur-js must be installed with Yarn: https://yarnpkg.com/')\""
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "deepmerge": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@types/jest": "^24.0.18",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "standard-version": "^6.0.1",
    "trash-cli": "^3.0.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": ".nyc_output",
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/*\\.test\\.{,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "modulePaths": [],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "jsx"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.test.js"
    ]
  }
}
